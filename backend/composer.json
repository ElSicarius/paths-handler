{
    "name": "example/path-handler",
    "description": "Serveur de endpoints dynamiques pour SSRF/Pentest, avec Lumen",
    "type": "project",
    "require": {
        "php": "^8.2",
        "laravel/lumen-framework": "^9.0",
        "vlucas/phpdotenv": "^5.5",
        "illuminate/view": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "stable"
}